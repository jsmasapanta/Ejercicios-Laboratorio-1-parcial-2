def potencia(x,y):
    '''
    Funcion que calcula la potencia de dos numeros

    Parametros:
    ------------
        x : float
        y : float

    Retorna:
    ------------
        potencia : float
    '''
    #Fomula para calcular la potencia
    potencia = x**y
    return potencia
def leer_variables():
    '''
    Funcion que lee 1 o mas variables dependiendo del usuario

    Parametros:
    ------------
        La funcion no tiene parametros

    Retorna:
    ------------
        x : float
        y : float
    '''
    #ingreso el valor de las variables X y Y
    x=float(input("Ingrese x:"))
    y=float(input("Ingrese y:"))
    
    return (x,y)
    
if __name__ == '__main__':
    print("HALLAR LA POTENCIA DE CUALQUIER NUMERO X^Y")
    #leemos x,y
    x,y=leer_variables()
    #llamamos a la funcion para calclular la velocidad
    potencia= potencia(x,y)
    #imprimo la potencia de dos numeros
    print("X^Y: ",potencia)

